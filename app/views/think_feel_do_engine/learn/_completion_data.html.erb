<% present({ event: lesson_event, lesson_module: ContentModules::LessonModule }, ThinkFeelDoEngine::LessonEvent::CompletionDataPresenter) do |presenter| %>
  <tr>
    <td class="not-displayed">
      <%= presenter.selected_at.to_i %>
    </td>
    <td class="not-displayed">
      <%= presenter.last_page_opened_at.to_i %>
    </td>
    <td>
      <%= presenter.pretty_title %>
    </td>
    <td>
      <%= presenter.page_headers %>
    </td>
    <td>
      <%= presenter.selected_at.to_s(:with_seconds) %>
    </td>
    <td>
      <%= presenter.last_page_number_opened %>
    </td>
    <td>
      <%= presenter.last_page_opened_at.to_s(:with_seconds) %>
    </td>
    <td>
      <%= distance_of_time_in_words(presenter.selected_at, presenter.last_page_opened_at, include_seconds: true) %>
    </td>
  </tr>
<% end %>