<tr>
  <td class="not-displayed">
    <%= task_status.completed_at ? task_status.completed_at.to_i : 0 %>
  </td>
  <td><%= task_status.task.title %></td>
  <td><%= (task_status.membership.start_date + task_status.start_day - 1).to_s(:user_date) %></td>
  <td><%= task_status.completed_at? ? task_status.completed_at.to_s(:standard) : "<span class='label label-warning'>Incomplete</span>".html_safe %></td>
</tr>
