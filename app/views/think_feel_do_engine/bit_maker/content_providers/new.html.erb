<h1>New Content Provider</h1>

<%= form_for(@content_provider, as: :content_provider, url: arm_bit_maker_content_providers_path(@arm, @content_provider)) do |f| %>
  
  <%= render 'form', f: f %>

  <div class="btn-toolbar">
    <%= f.submit "Create", class: "btn btn-default" %>
    <%= link_to "Cancel", (@content_provider.bit_core_content_module_id ? arm_bit_maker_content_module_path(@arm, @content_provider.bit_core_content_module_id) : arm_bit_maker_content_providers_path(@arm)), class: "btn btn-default" %>
  </div>

<% end %>